<p id="notice"><%= notice %></p>

<h1>Listing Rollbacks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rollbacks.each do |rollback| %>
      <tr>
        <td><%= link_to 'Show', rollback %></td>
        <td><%= link_to 'Edit', edit_rollback_path(rollback) %></td>
        <td><%= link_to 'Destroy', rollback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rollback', new_rollback_path %>
